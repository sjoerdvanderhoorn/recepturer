{
    "recipes": [{
            "id": "mac---cheese",
            "title": "Mac & Cheese",
            "description": "Home made alternative to boxed mac and cheese.",
            "directions": "\n* Boil 600 gram macaroni\n* Melt 20 gram butter&nbsp;in a saucepan over medium heat and add 5 gram flour, 2 gram peper, and 2 gram salt\n* Add 200 gram cheese&nbsp;to the macaroni\n* Mix",
            "for": "4",
            "people": 2
        },
        {
            "id": "lasagna",
            "title": "Lasagna",
            "description": "Family favorite.",
            "directions": "\n* Bake 500 gram ground-beef\n* Add 1 cup water\n* Add 2 teaspoons garlic-powder\n* Add 3 gram salt&nbsp;and 2 gram peper\n* Add 1 liter sauce\n* Take 1 box lasagna-noodles&nbsp;and lay it out in a bake tray, cover it with sauce, and repeat\n* Add 200 gram cheese&nbsp;to top it up\n* Put in over for 40 minutes",
            "for": "4",
            "people": 2
        },
        {
            "id": "spaghetti-bolognese",
            "title": "Spaghetti bolognese",
            "description": "Italian's best.",
            "directions": "\n* Boil 800 gram spaghetti\n* Bake 400 gram ground-beef\n* Add 300 gram sauce\n* Mix it up and serve",
            "for": "4",
            "people": 2
        }
